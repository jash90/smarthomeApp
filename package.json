{
  "name": "smarthome",
  "version": "1.2.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "clear": "rm -rf node_modules/ ios/Pods/ && npm install && cd ios && pod install && ..",
    "clearandroid": "rm -rf node_modules/ && npm install && react-native run-android",
    "clearios": "rm -rf node_modules/ ios/Pods/ && npm install && cd ios && pod install && .. && react-native run-ios",
    "pod": "cd ios && pod install && ..",
    "pod-update": "cd ios && pod update && pod install && ..",
    "android-release": "cd android && ./gradlew assembleRelease && ..",
    "postinstall": "patch-package && jetify",
    "patch": "yarn version --patch",
    "minor": "yarn version --minor",
    "major": "yarn version --major",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.8.1",
    "@react-native-community/netinfo": "^4.2.2",
    "@react-native-community/slider": "^2.0.8",
    "axios": "^0.19.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "moment": "^2.24.0",
    "native-base": "^2.13.8",
    "patch-package": "^6.2.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-simple-toast": "^0.1.1",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.6",
    "react-navigation-stack": "^1.8.0",
